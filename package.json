{
	"name": "misskey",
	"version": "13.14.2",
	"packageManager": "pnpm@8.10.2",
	"private": true,
	"scripts": {
		"build-pre": "node ./scripts/build-pre.js",
		"build": "turbo run build && pnpm gulp",
		"build-storybook": "pnpm --filter frontend build-storybook",
		"start": "pnpm check:connect && cd packages/backend && node ./built/boot/index.js",
		"start:test": "cd packages/backend && cross-env NODE_ENV=test node ./built/boot/index.js",
		"init": "pnpm migrate",
		"migrate": "pnpm -C packages/backend migrate",
		"check:connect": "pnpm -C packages/backend check:connect",
		"migrateandstart": "pnpm migrate && pnpm start",
		"gulp": "gulp build",
		"watch": "pnpm dev",
		"dev": "node ./scripts/dev.mjs",
		"lint": "turbo run lint",
		"cy:open": "cypress open --browser --e2e --config-file=cypress.config.ts",
		"cy:run": "cypress run",
		"e2e": "start-server-and-test start:test http://localhost:61812 cy:run",
		"jest": "pnpm -C packages/backend jest",
		"jest-and-coverage": "pnpm -C packages/backend jest-and-coverage",
		"test": "turbo run test",
		"test-and-coverage": "turbo run test-and-coverage",
		"format": "gulp format"
	},
	"resolutions": {
		"chokidar": "3.5.3",
		"lodash": "4.17.21"
	},
	"dependencies": {
		"execa": "^8.0.1",
		"gulp": "4.0.2",
		"gulp-cssnano": "2.1.3",
		"gulp-rename": "2.0.0",
		"gulp-replace": "1.1.4",
		"gulp-terser": "2.1.0",
		"js-yaml": "4.1.0"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.62",
		"@swc/core": "^1.3.96",
		"@tsconfig/node-lts": "^18.12.5",
		"@tsconfig/strictest": "^2.0.2",
		"@types/gulp": "4.0.10",
		"@types/gulp-rename": "2.0.1",
		"@typescript-eslint/eslint-plugin": "^6.10.0",
		"@typescript-eslint/parser": "^6.10.0",
		"cross-env": "7.0.3",
		"cypress": "12.17.1",
		"eslint": "^8.53.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "npm:eslint-plugin-i@^2.29.0",
		"prettier": "^3.0.3",
		"start-server-and-test": "2.0.0",
		"turbo": "^1.10.16",
		"typescript": "^5.2.2",
		"vitest": "0.33.0"
	},
	"optionalDependencies": {
		"@tensorflow/tfjs-core": "4.4.0"
	}
}
